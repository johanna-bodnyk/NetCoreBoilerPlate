{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Literate", "Serilog.Sinks.SumoLogic" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Fatal",
        "System": "Warning"
      }
    },
    "WriteTo": [
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "NetCoreSample"
    }
  }
}